{
  "expo": {
    "name": "StopShorts",
    "slug": "stopshorts",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "stopshorts",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E3A5F"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.stopshorts.app",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "アプリの使用状況を分析し、より良い体験を提供するために使用します。",
        "NSCameraUsageDescription": "自分の顔を見て目標を思い出すために、カメラへのアクセスが必要です。",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1E3A5F"
      },
      "package": "com.stopshorts.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-dev-client",
      "expo-camera",
      "./modules/screen-time/plugin/withScreenTime",
      "./modules/detox/plugin/withDetox"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0c46ad4b-81b5-4c52-a35e-aca3b18fca50"
      }
    },
    "owner": "webappai"
  }
}
